Package: touch
Title: Tools of Utilization and Cost in Healthcare
Version: 0.1-4.9004
Date: 2018-10-12
Authors@R: c(
    person(given = "Yan", family = "Li",
           email = "yan.4.li@uconn.edu",
           role = c("aut", "cre")),
    person(given = "Wenjie", family = "Wang", role = "aut",
           comment = c(ORCID = "0000-0003-0363-3180")),
    person(given = "Jun", family = "Yan", role = "aut")
